{
  "version": 2,
  "builds": [
    {
      "src": "./package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "command": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}